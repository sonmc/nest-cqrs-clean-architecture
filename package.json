{
  "name": "migrations-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "ts-node --require tsconfig-paths/register ./node_modules/typeorm/cli",
    "dbupdate": "npm run typeorm migration:run -- -d src/infrastructure/configuration/typeorm.config.ts",
    "migration": "npm run typeorm -- -d src/infrastructure/configuration/typeorm.config.ts migration:generate src/infrastructure/migrations/init",
    "migration:create": "npm run typeorm -- migration:create src/infrastructure/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- -d src/infrastructure/typeorm.config.ts migration:revert",
    "migration:show": "npm run typeorm -- migration:show -d src/infrastructure/typeorm.config.ts",
    "command": "ts-node -r tsconfig-paths/register ./src/infrastructure/configuration/commander.config.ts"
  },
  "dependencies": {
    "@automapper/classes": "^8.8.1",
    "@automapper/core": "^8.8.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^2.3.2",
    "@nestjs/core": "^9.0.0",
    "@nestjs/cqrs": "^10.2.7",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^9.0.1",
    "@types/bcrypt": "^5.0.2",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cookie-parser": "^1.4.6",
    "glob": "^11.0.0",
    "glob-fs": "^0.1.7",
    "mysql2": "^3.10.1",
    "nest-commander": "^3.13.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.2.0",
    "typeorm": "^0.3.16"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.4.3",
    "@types/express": "^4.17.13",
    "@types/jest": "^29.5.1",
    "@types/node": "18.16.12",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.7.0",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "^29.2.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.0.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "preset": "ts-jest",
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "coveragePathIgnorePatterns": [
      "src/app.module.ts",
      "src/main.ts",
      "src/application/commands/index.ts",
      "src/application/queries/index.ts",
      "src/domain/schemas/.*\\.ts",
      "src/domain/schemas/.*\\.schema\\.ts",
      "src/domain/helper/.*\\.ts",
      "src/domain/util/.*\\.ts",
      "src/infrastructure/config/.*\\.ts",
      "src/infrastructure/configuration/.*\\.ts",
      "src/infrastructure/services/.*\\.ts",
      "src/infrastructure/commander/.*\\.ts",
      "src/presentation/routes/.*\\.ts",
      "src/presentation/controller/.*/.*\\.presenter\\.ts",
      "src/presentation/middleware/.*\\.ts"
    ],
    "moduleNameMapper": {
      "^@commands/(.*)$": "<rootDir>/application/commands/$1",
      "^@queries/(.*)$": "<rootDir>/application/queries/$1",
      "^@domain/(.*)$": "<rootDir>/domain/$1",
      "^@services/(.*)$": "<rootDir>/infrastructure/services/$1"
    }
  }
}
